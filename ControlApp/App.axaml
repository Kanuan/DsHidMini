<Application xmlns="https://github.com/avaloniaui"
			 xmlns:wpf="using:Nefarius.DsHidMini.ControlApp.Util.WPF"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
			 xmlns:devmvvm="using:Nefarius.DsHidMini.ControlApp.MVVM"
             x:Class="ControlApp.App"
			 RequestedThemeVariant="Dark">
	
	<Application.Styles>
        <FluentTheme />
    </Application.Styles>
	<Application.Resources>
		<ResourceDictionary>
			<ResourceDictionary.MergedDictionaries>
				<ResourceInclude Source="/UI/GroupSettingsTemplates.axaml"/>
			</ResourceDictionary.MergedDictionaries>
		</ResourceDictionary>
		<wpf:VisibilityPerHidModeConverter x:Key="VisibilityPerHIDModeConverterKey"/>
	</Application.Resources>
</Application>
